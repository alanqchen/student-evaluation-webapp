<div class="w-full">
  <%= turbo_frame_tag :dashboardNav, class: "w-full block text-sm font-medium text-center text-gray-500 border-b border-gray-200" do %>
    <ul class="flex flex-wrap -mb-px">
      <li class="mr-2">
        <%= link_to  "Dashboard", dashboard_path, data: {turbo_frame: "dashboardContent",  action: "turbo:click->navigation#displayLoading", navigation_target: "link"}, class: "active text-blue-600 border-blue-600 inline-block p-4 rounded-t-lg border-b-2 border-transparent hover:text-gray-600 hover:border-gray-300" %>
      </li>
      <li class="mr-2">
        <%= link_to  "Settings", dashboard_settings_path, data: {turbo_frame: "dashboardContent",  action: "turbo:click->navigation#displayLoading", navigation_target: "link"}, class: "inline-block p-4 rounded-t-lg border-b-2 border-transparent hover:text-gray-600 hover:border-gray-300" %>
      </li>
      <% if current_user.admin %>
        <li class="mr-2">
          <a href="#" class="inline-block p-4 rounded-t-lg border-b-2 border-transparent hover:text-gray-600 hover:border-gray-300">Manage Users</a>
        </li>
      <% end %>
      <% if current_user.approver %>
        <li class="mr-2">
          <a href="#" class="inline-block p-4 rounded-t-lg border-b-2 border-transparent hover:text-gray-600 hover:border-gray-300">Manage Requests</a>
        </li>
      <% end %>
    </ul>
  <% end %>
</div>
